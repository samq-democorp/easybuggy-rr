{ 
   "packageRules": [
     {
      	 "groupName": "minor and patch",
      	 "matchUpdateTypes": ["minor", "patch"],
      	 "matchConfidence": ["very high", "high"],
      	 "automerge": true
     },
     {
      	 "matchUpdateTypes": ["major"],
      	 "matchConfidence": ["very high"],
